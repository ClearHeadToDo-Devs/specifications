{
  "@context": {
    "@vocab": "https://clearhead.us/vocab/actions/v3#",
    "schema": "http://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",
    "type": "@type",

    "actions": "@graph",

    "name": "schema:name",
    "description": "schema:description",
    "state": { "@id": "hasState", "@type": "@vocab" },
    "priority": { "@id": "hasPriority", "@type": "xsd:integer" },
    "story": "hasProject",
    "contexts": "requiresContext",

    "doDateTime": "hasDoDateTime",
    "doDuration": "hasDurationMinutes",
    "completedDate": "hasCompletedDateTime",
    "createdDate": "schema:dateCreated",
    "parent_id": { "@id": "parentAction", "@type": "@id" },

    "recurrence": "@nest",
    "frequency": "hasRecurrenceFrequency",
    "interval": "hasRecurrenceInterval",
    "count": "hasRecurrenceCount",
    "until": "hasRecurrenceUntil",
    "by_second": "bySecond",
    "by_minute": "byMinute",
    "by_hour": "byHour",
    "by_day": "byDay",
    "by_month_day": "byMonthDay",
    "by_year_day": "byYearDay",
    "by_week_no": "byWeekNo",
    "by_month": "byMonth",
    "by_set_pos": "bySetPos",
    "week_start": "weekStart",

    "predecessors": { "@id": "dependsOn", "@type": "@id" },

    "not_started": "NotStarted",
    "in_progress": "InProgress",
    "completed": "Completed",
    "blocked": "Blocked",
    "cancelled": "Cancelled"
  }
}